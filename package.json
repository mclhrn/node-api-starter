{
  "name": "node-temp",
  "version": "1.0.0",
  "description": "Starter Template for nodejs API on Openshift",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "ts-node build.ts && tsc",
    "dev": "nodemon src/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' src/index.ts | pino-pretty",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "sonar:start": "docker rm SonarQube && docker run -d --name SonarQube -p 9000:9000 -p 9092:9092 sonarqube",
    "sonar:inspect": "node sonar-project.ts"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^2.2.0",
    "mongoose": "^5.7.3",
    "pino": "^5.13.1",
    "pino-pretty": "^3.2.1",
    "prom-client": "^11.5.3",
    "request": "^2.88.0",
    "swagger-express-middleware": "^2.0.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/pino": "^5.8.8",
    "@types/swagger-express-middleware": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.3",
    "shelljs": "^0.8.3",
    "sonarqube-scanner": "^2.5.0",
    "supertest": "^4.0.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.6.2"
  },
  "author": "Red Hat App Dev Tech Domains"
}
