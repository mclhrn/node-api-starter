{
  "name": "node-temp",
  "version": "1.0.0",
  "description": "Starter Template for nodejs API on Openshift",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "ts-node build.ts && tsc",
    "dev": "nodemon src/index.ts",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' src/index.ts | pino-pretty",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "pino": "^5.17.0",
    "pino-pretty": "^3.6.1",
    "prom-client": "^11.5.3",
    "request": "^2.88.2",
    "swagger-ui-express": "^4.1.6",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/chai": "^4.2.22",
    "@types/express": "^4.17.13",
    "@types/mocha": "^5.2.7",
    "@types/pino": "^5.8.8",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chai": "^4.3.4",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.4",
    "shelljs": "^0.8.4",
    "supertest": "^4.0.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.10"
  },
  "author": "Red Hat App Dev Tech Domains"
}
